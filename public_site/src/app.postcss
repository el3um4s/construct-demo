@tailwind base;
/* Write your global styles here, in PostCSS syntax */

@tailwind components;
@tailwind utilities;

::-webkit-scrollbar {
	width: 7px;
	height: 7px;
}

::-webkit-scrollbar-track {
	background: #2d3748;
}

::-webkit-scrollbar-thumb {
	background: #cbd5e0;
}

::-webkit-scrollbar-thumb:hover {
	background: #718096;
}

* {
	color: #d1d5db;
}
